    "web": {
      "favicon": "./assets/favicon.png"
    },
    "scheme": "myon-health",
    "platforms": ["ios", "android"],
    "extra": {
      "eas": {
        "projectId": "f9c07c7d-ab5b-4f93-9a4a-5c8b5b4472fb"
