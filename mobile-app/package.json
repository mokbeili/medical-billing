{
  "name": "mobile-app",
  "version": "1.0.0",
  "main": "index.ts",
  "scripts": {
    "start": "expo start",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "build:preview": "chmod +x scripts/build-preview.sh && ./scripts/build-preview.sh",
    "build:preview:ios": "eas build --platform ios --profile preview",
    "build:preview:android": "eas build --platform android --profile preview",
    "build:production": "eas build --platform all --profile production",
    "setup:env": "cp .env.example .env && echo 'Please edit .env with your AWS credentials'"
  },
  "dependencies": {
    "@auth/prisma-adapter": "^2.10.0",
    "@expo/vector-icons": "^14.1.0",
